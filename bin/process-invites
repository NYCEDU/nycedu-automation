#!/usr/bin/env node

const getUsersCount = require('../db-helpers').getUsersCount,
 getInvitees = require('../get-invitees'),
 inviteAndFollowUsers = require('../invite-and-follow-users')

getUsersCount( function (numUsers) {
    getInvitees( numUsers, function (users) {
        inviteAndFollowUsers(users)
    })
})
